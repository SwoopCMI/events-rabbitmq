# Long job queue alert customization (optional)
# Comma-separated list of long-running queue names
LONG_JOB_QUEUES=colateral-events-production-location-ranges.delay,colateral-events-production-location-ranges
# Alert threshold for long job queues
LONG_JOB_QUEUE_THRESHOLD=1000000
# Cooldown (seconds) for long job queue alerts
LONG_JOB_QUEUE_COOLDOWN=10800
# Default cooldown (seconds) for other queues
DEFAULT_ALERT_COOLDOWN=1800
# RabbitMQ Configuration
RABBITMQ_DEFAULT_USER=rabbitmq
RABBITMQ_DEFAULT_PASS=testpassword123
RABBITMQ_ERLANG_COOKIE=secret-cookie-change-in-production

# Monitoring Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=15672

# Slack Webhook (optional for local testing)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# Alert Thresholds
ALERT_MAX_QUEUE_LENGTH=1000
ALERT_MAX_UNACKED_MESSAGES=500
ALERT_MIN_CONSUMERS=1
ALERT_MAX_MEMORY_PERCENT=80
ALERT_MAX_DISK_PERCENT=85
ALERT_PROCESSING_HALT_THRESHOLD=100
MONITORING_INTERVAL=60
